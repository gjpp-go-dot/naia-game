config_version=5

[application]

config/name="naia-game"
run/main_scene="res://scenes/Main.tscn"
config/features=PackedStringArray("4.0", "Mobile")
config/icon="res://icon.svg"

[autoload]

Dialogic="*res://addons/dialogic/Other/DialogicGameHandler.gd"

[dialogic]

animations/join_default_length=0.5
animations/leave_default_length=0.5
animations/leave_default_wait=true
animations/join_default_wait=true
choices/autofocus_first=true
choices/delay=0.2
glossary/default_case_sensitive=true
history/history_system=true
history/full_history=true
history/text_history=true
save/autosave_delay=60.0
text/speed=0.01
text/skippable=true
text/autocontinue_delay=1.0
variables={}
editor/current_editor=&"Timeline Editor"
editor/current_resources={
"Character Editor": "res://sources/hud/dialog/player.dch",
"Timeline Editor": "res://sources/hud/dialog/timeline.dtl"
}
editor/last_resources=["res://sources/hud/dialog/player.dch"]
editor/timeline_editor_mode=0
editor/character_preview_mode=1
editor/current_timeline_path="res://sources/hud/dialog/timeline.dtl"

[display]

window/size/viewport_width=1280
window/size/viewport_height=720
window/size/window_width_override=1152
window/size/window_height_override=648
window/stretch/mode="viewport"

[editor]

export/convert_text_resources_to_binary=true

